@model ArquivoViewModel

<div class="panel-body">
    <div class="form-horizontal">        
        <div class="form-group">
            <label asp-for="Id" class="control-label col-md-1"></label>
            <div class="col-md-11">
                @Html.EditorFor(model => model.Id, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
            </div>
        </div>
        <div class="form-group">
            <label asp-for="Tabela" class="control-label col-md-1"></label>
            <div class="col-md-11">
                @Html.EditorFor(model => model.Tabela, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
            </div>
        </div>
        <div class="form-group">
            <label asp-for="Esquema" class="control-label col-md-1"></label>
            <div class="col-md-11">
                @Html.EditorFor(model => model.Esquema, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
            </div>
        </div>
        <div class="form-group">
            <label asp-for="FrequenciaExecucao.Frequencia" class="control-label col-md-1"></label>
            <div class="col-md-11">
                @Html.EditorFor(model => model.FrequenciaExecucao.Frequencia, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
            </div>
        </div>
    </div>
</div>